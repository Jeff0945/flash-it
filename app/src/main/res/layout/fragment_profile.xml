<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A1D9D7">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="450dp"
        android:layout_height="350dp"
        android:layout_marginTop="-200dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.139"
        app:srcCompat="@drawable/header" />

    <ImageView
        android:id="@+id/profile_card01"
        android:layout_width="400dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.33"
        app:srcCompat="@drawable/rectangle" />

    <ImageView
        android:id="@+id/circularImage"
        android:layout_width="86dp"
        android:layout_height="85dp"
        android:src="@drawable/no_profile_image"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.057" />

    <TextView
        android:id="@+id/settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Settings"
        android:textColor="#179495"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@+id/profile_card01"
        app:layout_constraintEnd_toEndOf="@+id/profile_card01"
        app:layout_constraintHorizontal_bias="0.284"
        app:layout_constraintStart_toStartOf="@+id/profile_card01"
        app:layout_constraintTop_toTopOf="@+id/profile_card01"
        app:layout_constraintVertical_bias="0.494" />

    <ImageView
        android:id="@+id/setting_icon"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:src="@drawable/settings"
        app:layout_constraintBottom_toBottomOf="@+id/profile_card01"
        app:layout_constraintEnd_toEndOf="@+id/profile_card01"
        app:layout_constraintHorizontal_bias="0.097"
        app:layout_constraintStart_toStartOf="@+id/profile_card01"
        app:layout_constraintTop_toTopOf="@+id/profile_card01"
        app:layout_constraintVertical_bias="0.499"
        app:tint="#179495" />

    <ImageView
        android:id="@+id/toggle01"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/leftarrow"
        app:layout_constraintBottom_toBottomOf="@+id/profile_card01"
        app:layout_constraintEnd_toEndOf="@+id/profile_card01"
        app:layout_constraintHorizontal_bias="0.917"
        app:layout_constraintStart_toStartOf="@+id/profile_card01"
        app:layout_constraintTop_toTopOf="@+id/profile_card01"
        app:layout_constraintVertical_bias="0.487"
        app:tint="#179495" />

    <!--  Card 02  -->

    <ImageView
        android:id="@+id/profile_card02"
        android:layout_width="400dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_card01"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangle" />

    <TextView
        android:id="@+id/feedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Feedback"
        android:textColor="#179495"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@+id/profile_card02"
        app:layout_constraintEnd_toEndOf="@+id/profile_card02"
        app:layout_constraintHorizontal_bias="0.285"
        app:layout_constraintStart_toStartOf="@+id/profile_card02"
        app:layout_constraintTop_toTopOf="@+id/profile_card02"
        app:layout_constraintVertical_bias="0.479" />

    <ImageView
        android:id="@+id/feedback_icon"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:src="@drawable/feedback"
        app:layout_constraintBottom_toBottomOf="@+id/profile_card02"
        app:layout_constraintEnd_toEndOf="@+id/profile_card02"
        app:layout_constraintHorizontal_bias="0.096"
        app:layout_constraintStart_toStartOf="@+id/profile_card02"
        app:layout_constraintTop_toTopOf="@+id/profile_card02"
        app:layout_constraintVertical_bias="0.50"
        app:tint="#179495" />

    <ImageView
        android:id="@+id/toggle02"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/leftarrow"
        app:layout_constraintBottom_toBottomOf="@+id/profile_card02"
        app:layout_constraintEnd_toEndOf="@+id/profile_card02"
        app:layout_constraintHorizontal_bias="0.914"
        app:layout_constraintStart_toStartOf="@+id/profile_card02"
        app:layout_constraintTop_toTopOf="@+id/profile_card02"
        app:layout_constraintVertical_bias="0.49"
        app:tint="#179495" />

    <TextView
        android:id="@+id/profile_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="Guest"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/circularImage"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.077" />

    <TextView
        android:id="@+id/profile_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="guest@guest.com"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/circularImage"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.104" />

